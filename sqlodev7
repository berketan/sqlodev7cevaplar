SORU 1;
SELECT rating
FROM film 
GROUP BY rating;

SORU 2;
SELECT  replacement_cost, COUNT(replacement_cost)
FROM film 
GROUP BY replacement_cost
HAVING COUNT(replacement_cost). 50;

SORU 3;
SELECT COUNT(customer_id),  store_id
FROM customer
GROUP BY store_id ;

SORU 4;

SELECT country_id, COUNT(city_id)
FROM city 
GROUP BY country_id , city_id
HAVING COUNT(city_id)= MAX(city_id);
